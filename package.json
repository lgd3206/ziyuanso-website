    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir ."
  },
  "keywords": [
    "网盘搜索",
    "资源搜索",
    "百度网盘",
    "阿里云盘",
    "夸克网盘",
    "静态网站"
  ],
  "author": "ziyuanso.net",
  "license": "MIT",
  "devDependencies": {
    "serve": "^14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ziyuanso-website.git"
  },
  "homepage": "https://www.ziyuanso.net",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "index.html",
    "assets/**/*",
    "pages/**/*",
    "*.json",
    "*.toml",
    "README.md"
  ]
}
